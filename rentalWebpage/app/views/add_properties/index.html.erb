<p id="notice"><%= notice %></p>

<h1>Listing Add Properties</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Apartment</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Rooms</th>
      <th>Bathrooms</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @add_properties.each do |add_property| %>
      <tr>
        <td><%= add_property.address %></td>
        <td><%= add_property.apartment %></td>
        <td><%= add_property.city %></td>
        <td><%= add_property.state %></td>
        <td><%= add_property.zipcode %></td>
        <td><%= add_property.rooms %></td>
        <td><%= add_property.bathrooms %></td>
        <td><%= link_to 'Show', add_property %></td>
        <td><%= link_to 'Edit', edit_add_property_path(add_property) %></td>
        <td><%= link_to 'Destroy', add_property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Add property', new_add_property_path %>
